<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.iutlpweb.fr.happyshop" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/grocery_cart"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".views.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".views.ListCatActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".views.ListProdActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".views.ModCatActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".views.ModProdActivity"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>
